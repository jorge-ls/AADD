	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>si</from-outcome>
			<to-view-id>/bienvenido.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>no</from-outcome>
			<to-view-id>/falloLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>registrar</display-name>
		<from-view-id>/registrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>si</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>registrar</display-name>
		<from-view-id>/registrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>no</from-outcome>
			<to-view-id>/falloRegistrar.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>falloRegistrar</display-name>
		<from-view-id>/falloRegistrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>registrar</from-outcome>
			<to-view-id>/registrar.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>falloRegistrar</display-name>
		<from-view-id>/falloRegistrar.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
