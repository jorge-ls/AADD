	<h:form>
    <p:panel id="panel" header="Añadir coche" style="width:400px;">
 
<!--         <p:messages id="msgs" /> -->
 		<p:growl id="growl" sticky="true" />
        <h:panelGrid columns="3" cellpadding="1">
            <p:outputLabel for="matricula" value="Matricula:" />
            <p:inputText id="matricula" value="#{beanAnyadirCoche.matricula}" required="true" label="Matricula"/>
            <p:message for="matricula" display="icon" />
 
            <p:outputLabel for="modelo" value="Modelo:" />
            <p:inputText id="modelo" value="#{beanAnyadirCoche.modelo}" label="Modelo" required="true"/>
            <p:message for="modelo"  display="icon" />
            
            <p:outputLabel for="año" value="Año:" />
            <p:inputText id="año" value="#{beanAnyadirCoche.anyo}" label="Año" required="true"/>
            
            <p:message for="año" display="icon" />
            
            <p:outputLabel for="confort" value="Confort:" />
            <p:inputText id="confort" value="#{beanAnyadirCoche.confort}" label="Confort" required="true"/>
            <p:message for="confort" display="icon" />
 
        </h:panelGrid>
 
        <p:commandButton value="Añadir" update="panel" action="#{beanAnyadirCoche.anyadirCoche}" icon="pi pi-check" />
    </p:panel>
</h:form>
